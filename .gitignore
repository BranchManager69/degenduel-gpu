# Ignore nested git repositories 
/repos/                            # Contains all repos that didi RAG system uses, including didi itself
/vanity-grinder                    # GPU-accelerated Solana vanity address generator
/.vast/                            # Lambda/Vast cloud provider directories

# Exclude any future Didi installation at the top level
/didi/                             # In case didi is installed at the top level

# Exclude data contents except the directory itself
/data/*
!/data/.gitkeep

# Exclude models contents except the directory itself
/models/*
!/models/.gitkeep

# Node modules and dependencies
node_modules/
__pycache__/
*.pyc
venv/
venvs/

# Build artifacts
target/
dist/
build/

# Rust specific
Cargo.lock
**/*.rs.bk

# Environment and configuration
.env
.env.local
.env.*.local

# IDE/Editor specific
.vscode/
.idea/
*.swp
*.swo

# OS specific
.DS_Store
Thumbs.db

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Temporary files
*.tmp
*.bak
*~
